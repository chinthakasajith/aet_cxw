// buttons
@aet-btn-primary-bg: lighten(@aet-brand-blue, 15%);

//header navbar
@navbar-brand-height :115px;


// Admin portal aside navbar
@left-sidebar-width: 260px;
@ggw: @grid-gutter-width;

@permit-field-font-size: 14px;
@permit-field-padding: 15px;
@permit-field-height: (@permit-field-font-size * @line-height-base) + (@permit-field-padding * 2);

// top navbar
@nav-bg-color : @gray-lighter;
@nav-brand-bg-color: @gray-lighter;
@nav-button-color: lighten(@nav-bg-color, 3%);
@nav-link-color: @aet-btn-primary-bg;

//admin portal aside admin navbar colors
@admin-sidebar-bgColor : @gray-lighter;
@admin-sidebar-active-bg : lighten(@aet-brand-green, 10%);
@admin-sidebar-active-fontColor : #fff;
@admin-sidebar-hover-color : #ddd;
@admin-sidebar-icon-color: @aet-brand-green;
@admin-sidebar-link-color: #5E5E5E;

// alerts
@alerts-success: lighten(@brand-success, 10%);
@alerts-error: lighten(@brand-danger, 10%);
@alerts-info: @gray-lighter;
@alerts-message:lighten(@brand-primary, 10%);