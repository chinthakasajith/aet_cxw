.mission-landing {
  #navigation {
    display: inline-block;
    button {
      padding: 20px;
      width: 196px;
      height: 66px;
      float: left;
      display: block;
      margin-top: 20px;
      margin-left: 5px;
      text-align: center;
      position: relative;
      background-color: #fff;
      border: 1px solid #adadad;
    }
    button:hover {
      background-color: #e6e6e6;
    }
    .navLabel {
      vertical-align: middle;
    }
    button.passed {
      padding: 20px;
      width: 196px;
      background-color: #aaa;
      border: 1px solid #666;
      color: #fff;
      cursor: default;
      display: block;
      margin-top: 20px;
      text-align: center;
      float: left;
    }
    .glyphicon {
      font-size: 24px;
    }
    .glyphicon-remove {
      background-color: #d9534f;
      border-color: #d43f3a;
      color: #fff;
      left: -4px;
      padding: 4px;
      position: absolute;
      top: -16px;
    }
    .glyphicon-ok {
      background-color: #a0d24e;
      border-color: #769b3b;
      color: #fff;
      left: -4px;
      padding: 4px;
      position: absolute;
      top: -16px;
    }
    .glyphicon-arrow-right {
      vertical-align: middle;
      font-size: 24px;
    }
  }
  .textAlignLeft {
    text-align: left;
    padding-top: 7px;
    margin-bottom: 0;
  }
  .btn-preview {
    width: auto;
    color: #fff;
  }
  .animate-if {
    background: white;
    padding: 10px;
  }
  .animate-if.ng-enter,
  .animate-if.ng-leave {
    transition: all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s;
  }
  .animate-if.ng-enter,
  .animate-if.ng-leave.ng-leave-active {
    opacity: 0;
  }
  .animate-if.ng-leave,
  .animate-if.ng-enter.ng-enter-active {
    opacity: 1;
  }
  .scrollable-list {
    height: auto;
    width: 800px;
    max-height: 180px;
    overflow-x: hidden;
  }
  .scrollable-list::-webkit-scrollbar {
    -webkit-appearance: none;
    width: 4px;
  }
  .scrollable-list::-webkit-scrollbar-thumb {
    border-radius: 3px;
    background-color: lightgray;
    -webkit-box-shadow: 0 0 1px rgba(255, 255, 255, .75);
  }
  .scrollable-listPT {
    height: auto;
    max-height: 180px;
    overflow-x: hidden;
  }
  .scrollable-listPT::-webkit-scrollbar {
    -webkit-appearance: none;
    width: 4px;
  }
  .scrollable-listPT::-webkit-scrollbar-thumb {
    border-radius: 3px;
    background-color: lightgray;
    -webkit-box-shadow: 0 0 1px rgba(255, 255, 255, .75);
  }
  .defaultscroll {
    max-height: 300px;
    overflow: auto;
    margin-bottom: 20px;
  }
  .margin20 {
    margin: 20px;
  }
  #updateForm fieldset {
    padding-top: 10px;
  }
  .tooltip-custom {
    margin-top: 4px;
  }
  .copy-mission {
    position: absolute;
    right: -150px;
    top: -1px;
  }
  .sort-by-label {
    label {
      min-width: 220px;
    }
  }
}
